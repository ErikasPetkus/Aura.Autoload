{
    "name": "aura/autoload",
    "type": "aura-package",
    "description": "The Aura Autoload package provides a PSR-4 compliant SPL autoloader implementation for PHP.",
    "keywords": ["PSR-0", "autoload", "autoloader", "SPL autoloader", "SplClassLoader", "class loader" ],
    "homepage": "http://auraphp.github.com/Aura.Autoload",
    "license": "BSD-2-Clause",
    "authors": [
        {
            "name": "Aura.Autoload Contributors",
            "homepage": "https://github.com/auraphp/Aura.Autoload/contributors"
        }
    ],
    "require": {
        "php": ">=5.4.0",
        "aura/installer-default": "1.0.0"
    },
    "autoload": {
        "psr-0": {
            "Aura\\Autoload": "src/"
        }
    }
}
